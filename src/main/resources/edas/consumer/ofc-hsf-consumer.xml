<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:hsf="http://www.taobao.com/hsf" xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
        http://www.taobao.com/hsf
        http://www.taobao.com/hsf/hsf.xsd"
       default-autowire="byName">

    <!-- uam 用户中心 -->
    <hsf:consumer id="uamAuthEdasService" interface="com.xescm.uam.provider.UamAuthEdasService" group="xescm-uam-${xescm.edas.group}" version="1.0" clientTimeout="60000"/>
    <hsf:consumer id="uamGroupEdasService" interface="com.xescm.uam.provider.UamGroupEdasService" group="xescm-uam-${xescm.edas.group}" version="1.0" clientTimeout="60000"/>
    <hsf:consumer id="uamUserEdasService" interface="com.xescm.uam.provider.UamUserEdasService" group="xescm-uam-${xescm.edas.group}" version="1.0" clientTimeout="60000"/>
    <hsf:consumer id="chatRobotEdasService" interface="com.xescm.uam.provider.ChatRobotEdasService" group="xescm-uam-${xescm.edas.group}" version="1.0" clientTimeout="60000"/>


    <!-- 客户中心 -->
    <hsf:consumer id="cscContactEdasService" interface="com.xescm.csc.provider.CscContactEdasService" group="xescm-csc-${xescm.edas.group}" version="1.0" clientTimeout="60000"/>
    <hsf:consumer id="cscCustomerEdasService" interface="com.xescm.csc.provider.CscCustomerEdasService"  group="xescm-csc-${xescm.edas.group}" version="1.0" clientTimeout="60000"/>
    <hsf:consumer id="cscGoodsEdasService" interface="com.xescm.csc.provider.CscGoodsEdasService"  group="xescm-csc-${xescm.edas.group}" version="1.0" clientTimeout="60000"/>
    <hsf:consumer id="cscGoodsTypeEdasService" interface="com.xescm.csc.provider.CscGoodsTypeEdasService"  group="xescm-csc-${xescm.edas.group}" version="1.0" clientTimeout="60000"/>
    <hsf:consumer id="cscStoreEdasService" interface="com.xescm.csc.provider.CscStoreEdasService"  group="xescm-csc-${xescm.edas.group}" version="1.0" clientTimeout="60000"/>
    <hsf:consumer id="cscSupplierEdasService" interface="com.xescm.csc.provider.CscSupplierEdasService"  group="xescm-csc-${xescm.edas.group}" version="1.0" clientTimeout="60000"/>
    <hsf:consumer id="cscWarehouseEdasService" interface="com.xescm.csc.provider.CscWarehouseEdasService"  group="xescm-csc-${xescm.edas.group}" version="1.0" clientTimeout="60000"/>
    <!-- 结算中心 -->
    <hsf:consumer id="acOrderEdasService" interface="com.xescm.ac.provider.AcOrderEdasService" group="xescm-ac-${xescm.edas.group}" version="1.0" clientTimeout="60000"/>
    <hsf:consumer id="acSettleStatisticsEdasService" interface="com.xescm.ac.provider.AcSettleStatisticsEdasService" group="xescm-ac-${xescm.edas.group}" version="1.0" clientTimeout="60000"/>


    <!-- 资源中心 -->
    <hsf:consumer id="rmcAddressEdasService" interface="com.xescm.rmc.edas.service.RmcAddressEdasService" group="xescm-rmc-${xescm.edas.group}" version="1.0" clientTimeout="600000"/>
    <hsf:consumer id="rmcCarEdasService" interface="com.xescm.rmc.edas.service.RmcCarEdasService" group="xescm-rmc-${xescm.edas.group}" version="1.0" clientTimeout="600000"/>
    <hsf:consumer id="rmcCompanyInfoEdasService" interface="com.xescm.rmc.edas.service.RmcCompanyInfoEdasService" group="xescm-rmc-${xescm.edas.group}" version="1.0" clientTimeout="600000"/>
    <hsf:consumer id="rmcServiceCoverageEdasService" interface="com.xescm.rmc.edas.service.RmcServiceCoverageEdasService" group="xescm-rmc-${xescm.edas.group}" version="1.0" clientTimeout="600000"/>
    <hsf:consumer id="rmcWarehouseEdasService" interface="com.xescm.rmc.edas.service.RmcWarehouseEdasService" group="xescm-rmc-${xescm.edas.group}" version="1.0" clientTimeout="600000"/>

    <!--Epc订单取消 -->
    <hsf:consumer id="epcOrderCancelEdasService" interface="com.xescm.epc.edas.service.EpcOrderCancelEdasService" group="xescm-epc-${xescm.edas.group}" version="1.0" clientTimeout="600000"/>
    <!-- Epc返回映射表-->
    <hsf:consumer id="epcRecordMappedEdasService" interface="com.xescm.epc.edas.service.EpcRecordMappedEdasService" group="xescm-epc-${xescm.edas.group}" version="1.0" clientTimeout="600000"/>
    <hsf:consumer id="epcOfc2DmsEdasService" interface="com.xescm.epc.edas.service.EpcOfc2DmsEdasService" group="xescm-epc-${xescm.edas.group}" version="1.0" clientTimeout="600000"/>

    <!--仓储中心-->
    <hsf:consumer id="whcOrderCancelEdasService" interface="com.xescm.whc.edas.service.WhcOrderCancelEdasService" group="xescm-whc-${xescm.edas.group}" version="1.0" clientTimeout="600000"/>

    <!--运输中心-->
    <hsf:consumer id="cancelOrderEdasService" interface="com.xescm.tfc.edas.service.CancelOrderEdasService" group="xescm-tfc-${xescm.edas.group}" version="1.0" clientTimeout="600000"/>
    <hsf:consumer id="tfcQueryEveryDeliveryService" interface="com.xescm.tfc.edas.service.TfcQueryEveryDeliveryService" group="xescm-tfc-${xescm.edas.group}" version="1.0" clientTimeout="600000"/>
</beans>